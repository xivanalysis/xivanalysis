{
  "name": "@xivanalysis/client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "test": "jest",
    "lint": "yarn run lint:js && yarn run lint:ts",
    "lint:js": "eslint src",
    "lint:ts": "tslint src/**/*.ts{,x}",
    "extract": "rimraf locale/_build && rimraf locale/en && lingui add-locale en && cross-env NODE_ENV=development lingui extract --clean",
    "compile": "lingui compile"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js{,x}": [
        "eslint --fix"
      ],
      "src/**/*.ts{,x}": [
        "tslint --fix"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lingui": {
    "fallbackLocale": "en",
    "sourceLocale": "en",
    "format": "minimal"
  },
  "resolutions": {
    "@babel/core": "^7.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@lingui/react": "^2.5.0",
    "@xivanalysis/tooltips": "xivanalysis/tooltips",
    "axios": "^0.18.0",
    "chart.js": "^2.7.2",
    "color": "^3.0.0",
    "es6-error": "^4.1.1",
    "intersection-observer": "^0.5.0",
    "intl": "^1.2.5",
    "lodash": "^4.17.10",
    "mathjs": "^5.1.1",
    "prop-types": "^15.6.1",
    "raven-js": "^3.26.4",
    "raw-loader": "^0.5.1",
    "react": "^16.5.2",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-intersection-observer": "^6.2.3",
    "react-markdown": "^4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-sizes": "^1.0.4",
    "react-visjs-timeline": "^1.5.0",
    "redux": "^4",
    "redux-thunk": "^2.2.0",
    "reflect-metadata": "^0.1.12",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.83",
    "stable": "^0.1.8",
    "toposort": "^2",
    "tslib": "^1.9.3",
    "url-search-params-polyfill": "^5.0.0",
    "vis": "^4.21.0"
  },
  "devDependencies": {
    "@lingui/babel-plugin-transform-js": "^2.5.0",
    "@lingui/babel-plugin-transform-react": "^2.5.0",
    "@lingui/cli": "^2.5.0",
    "@lingui/loader": "^2.5.0",
    "@neutrinojs/copy": "^9.0.0-beta.1",
    "@neutrinojs/eslint": "^9.0.0-beta.1",
    "@neutrinojs/jest": "^9.0.0-beta.1",
    "@neutrinojs/react": "^9.0.0-beta.1",
    "@types/chart.js": "^2.7.37",
    "@types/jest": "^23.3.3",
    "@types/lingui__react": "^2.5.0",
    "@types/lodash": "^4.14.116",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-scroll": "^1.5.4",
    "@types/webpack-env": "^1.13.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-macros": "^2.4.2",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "dotenv-expand": "^4.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-xivanalysis": "xivanalysis/linter",
    "glob": "^7.1.3",
    "husky": "^1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-chain": "^1.0.3",
    "jest-extended": "^0.10.0",
    "jest-junit": "^5.2.0",
    "lint-staged": "^7.3.0",
    "neutrino": "^9.0.0-beta.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.3",
    "ts-loader": "^5.2.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.1.1",
    "typings-for-css-modules-loader": "xivanalysis/typings-for-css-modules-loader",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpackbar": "^3.0.3"
  }
}
