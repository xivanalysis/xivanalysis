{
  "name": "@xivanalysis/client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "test": "jest",
    "lint": "yarn run lint:js && yarn run lint:ts",
    "lint:js": "eslint src",
    "lint:ts": "tslint src/**/*.ts{,x}",
    "extract": "rimraf locale/_build && rimraf locale/en && lingui add-locale en && cross-env NODE_ENV=development lingui extract --clean",
    "compile": "lingui compile"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js{,x}": [
        "eslint --fix"
      ],
      "src/**/*.ts{,x}": [
        "tslint --fix"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lingui": {
    "fallbackLocale": "en",
    "sourceLocale": "en",
    "format": "minimal"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@lingui/react": "^2.5.0",
    "@types/color": "^3.0.0",
    "@xivanalysis/tooltips": "^0.0.6",
    "chart.js": "^2.7.2",
    "classnames": "^2.2.6",
    "color": "^3.0.0",
    "es6-error": "^4.1.1",
    "intersection-observer": "^0.7.0",
    "intl": "^1.2.5",
    "ky": "^0.11.1",
    "lodash": "^4.17.13",
    "mathjs": "^5.1.1",
    "mobx": "^5.10.1",
    "mobx-react": "^6.0.3",
    "mobx-react-lite": "^1.4.0",
    "prop-types": "^15.6.1",
    "raven-js": "^3.26.4",
    "raw-loader": "^3.0.0",
    "react": "^16.8.0",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.8.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3",
    "react-router-dom": "^5.0.0",
    "react-visjs-timeline": "^1.5.0",
    "reflect-metadata": "^0.1.12",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.87",
    "stable": "^0.1.8",
